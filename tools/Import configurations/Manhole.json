{
  "target_layer": "manhole",
  "conversion_settings": {
    "use_snapping": true,
    "snapping_distance": 0.1,
    "create_connection_nodes": true,
    "create_manholes": false,
    "length_source_field": "",
    "length_fallback_value": 10.0,
    "azimuth_source_field": "",
    "azimuth_fallback_value": 90,
    "edit_channels": false
  },
  "fields": {
    "id": {
      "method": "auto",
      "source_attribute": ""
    },
    "code": {
      "method": "source_attribute",
      "source_attribute": "PUTNUMMER"
    },
    "display_name": {
      "method": "source_attribute",
      "source_attribute": "PUTNUMMER"
    },
    "calculation_type": {
      "method": "default",
      "source_attribute": "",
      "default_value": 2
    },
    "shape": {
      "method": "default",
      "source_attribute": "",
      "default_value": "02"
    },
    "width": {
      "method": "expression",
      "source_attribute": "",
      "expression": " array_get( string_to_array( replace( \"PUTAFMETING\" , 'mm', ''), 'x'), 0) / 1000"
    },
    "length": {
      "method": "expression",
      "source_attribute": "",
      "expression": " array_get( string_to_array( replace( \"PUTAFMETING\" , 'mm', ''), 'x'), 1) / 1000"
    },
    "bottom_level": {
      "method": "ignore",
      "source_attribute": ""
    },
    "surface_level": {
      "method": "expression",
      "source_attribute": "",
      "expression": "to_real(replace( \"F__UPUTDEKSELHOOGTE\" , '%%U', ''))"
    },
    "drain_level": {
      "method": "expression",
      "source_attribute": "",
      "expression": "to_real(replace( \"F__UPUTDEKSELHOOGTE\" , '%%U', '')) - 0.15"
    },
    "sediment_level": {
      "method": "ignore",
      "source_attribute": ""
    },
    "manhole_indicator": {
      "method": "default",
      "source_attribute": "",
      "default_value": 0
    },
    "zoom_category": {
      "method": "default",
      "source_attribute": "",
      "default_value": 3
    },
    "connection_node_id": {
      "method": "auto",
      "source_attribute": ""
    },
    "exchange_thickness": {
      "method": "ignore",
      "source_attribute": ""
    },
    "hydraulic_conductivity_in": {
      "method": "ignore",
      "source_attribute": ""
    },
    "hydraulic_conductivity_out": {
      "method": "ignore",
      "source_attribute": ""
    }
  },
  "connection_node_fields": {
    "id": {
      "method": "auto",
      "source_attribute": ""
    },
    "code": {
      "method": "source_attribute",
      "source_attribute": "PUTNUMMER"
    },
    "initial_waterlevel": {
      "method": "ignore",
      "source_attribute": ""
    },
    "storage_area": {
      "method": "expression",
      "source_attribute": "",
      "expression": "  array_get( string_to_array( replace( \"PUTAFMETING\" , 'mm', ''), 'x'), 0) / 1000 * array_get( string_to_array( replace( \"PUTAFMETING\" , 'mm', ''), 'x'), 1) / 1000 "
    }
  }
}