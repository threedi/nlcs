{
  "target_layer": "pipe",
  "conversion_settings": {
    "use_snapping": true,
    "snapping_distance": 0.1,
    "create_connection_nodes": true,
    "create_manholes": false
  },
  "fields": {
    "id": {
      "method": "auto",
      "source_attribute": ""
    },
    "code": {
      "method": "ignore",
      "source_attribute": ""
    },
    "display_name": {
      "method": "ignore",
      "source_attribute": ""
    },
    "calculation_type": {
      "method": "default",
      "source_attribute": "",
      "default_value": 1
    },
    "dist_calc_points": {
      "method": "default",
      "source_attribute": "",
      "default_value": 1000.0
    },
    "invert_level_start_point": {
      "method": "expression",
      "source_attribute": "",
      "expression": "to_real( replace( \"BOB_BEGIN\" ,'+','') ) * if(regexp_match(\"BOB_BEGIN\", '-'), -1, 1) * if(regexp_match(\"BOB_BEGIN\", '-'), -1, 1)"
    },
    "invert_level_end_point": {
      "method": "expression",
      "source_attribute": "",
      "expression": "to_real( replace(  \"BOB_EIND\"  ,'+','') ) * if(regexp_match( \"BOB_EIND\" , '-'), -1, 1) * if(regexp_match( \"BOB_EIND\" , '-'), -1, 1)"
    },
    "friction_value": {
      "method": "expression",
      "source_attribute": "",
      "expression": "CASE \r\n\tWHEN DIAMETER LIKE '%BETON%' THEN 0.0145\r\n\tWHEN DIAMETER LIKE '%GRES%' THEN 0.0115\r\n\tWHEN DIAMETER LIKE '%PE%' THEN 0.011\r\n\tWHEN DIAMETER LIKE '%PVC%' THEN 0.011\r\n\tWHEN DIAMETER LIKE '%GVK%' THEN 0.011\r\n\tWHEN DIAMETER LIKE '%PP%' THEN 0.011\r\nELSE 0.015\r\nEND\r\n\r\n/*\r\n\tWaarden die in processing algorithm Guess Indicators worden gebruikt:\r\n    TABLE_MANNING = {\r\n        (MATERIAL_TYPE_CONCRETE, 0.0145),\r\n        (MATERIAL_TYPE_PVC, 0.0110),\r\n        (MATERIAL_TYPE_STONEWARE, 0.0115),\r\n        (MATERIAL_TYPE_CAST_IRON, 0.0135),\r\n        (MATERIAL_TYPE_BRICKWORK, 0.0160),\r\n        (MATERIAL_TYPE_HPE, 0.0110),\r\n        (MATERIAL_TYPE_HPDE, 0.0110),\r\n        (MATERIAL_TYPE_SHEET_IRON, 0.0135),\r\n        (MATERIAL_TYPE_STEEL, 0.0130),\r\n    }\r\n*/"
    },
    "friction_type": {
      "method": "default",
      "source_attribute": "",
      "default_value": 2
    },
    "material": {
      "method": "expression",
      "source_attribute": "",
      "default_value": "NULL",
      "expression": "CASE \r\n\tWHEN DIAMETER LIKE '%BETON%' THEN 0\r\n\tWHEN DIAMETER LIKE '%GRES%' THEN 2\r\n\tWHEN DIAMETER LIKE '%PE%' THEN 5\r\n\tWHEN DIAMETER LIKE '%PVC%' THEN 1\r\n\tWHEN DIAMETER LIKE '%GVK%' THEN NULL\r\n\tWHEN DIAMETER LIKE '%PP%' THEN NULL\r\nELSE NULL\r\nEND\r\n\r\n-- 0: concrete; 1: pvc; 2: gres; 3: cast iron; 4: brickwork; 5: HPE; 6: HDPE; 7: plate irAon; 8: steel."
    },
    "sewerage_type": {
      "method": "source_attribute",
      "source_attribute": "nlcs_object",
      "value_map": {
        "ITR": 3,
        "VWA": 2,
        "DRAINAGE": 3,
        "DWA": 2,
        "GWA": 0,
        "HWA": 1,
        "OVERIG": 3,
        "SWA": 2
      },
      "default_value": 3
    },
    "zoom_category": {
      "method": "default",
      "source_attribute": "",
      "default_value": 3
    },
    "connection_node_start_id": {
      "method": "auto",
      "source_attribute": ""
    },
    "connection_node_end_id": {
      "method": "auto",
      "source_attribute": ""
    },
    "cross_section_shape": {
      "method": "default",
      "source_attribute": "",
      "default_value": 2
    },
    "cross_section_width": {
      "method": "expression",
      "source_attribute": "",
      "expression": "to_real(regexp_substr(\"DIAMETER\", 'C(\\\\d+)mm'))"
    },
    "cross_section_height": {
      "method": "ignore",
      "source_attribute": ""
    },
    "cross_section_table": {
      "method": "ignore",
      "source_attribute": ""
    },
    "exchange_thickness": {
      "method": "ignore",
      "source_attribute": ""
    },
    "hydraulic_conductivity_in": {
      "method": "ignore",
      "source_attribute": ""
    },
    "hydraulic_conductivity_out": {
      "method": "ignore",
      "source_attribute": ""
    }
  },
  "connection_node_fields": {
