      "source_attribute": "",
      "default_value": "NULL"
    },
    "zoom_category": {
      "method": "source_attribute",
      "source_attribute": "",
      "default_value": "NULL"
    },
    "connection_node_id": {
      "method": "auto",
      "source_attribute": ""
    },
    "exchange_thickness": {
      "method": "source_attribute",
